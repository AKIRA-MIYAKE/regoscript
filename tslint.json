{
  "rulesDirectory": [
    "tslint-plugin-prettier"
  ],
  "extends": [
    "tslint-config-standard",
    "tslint-config-prettier"
  ],
  "rules": {
    "prettier": [
      true,
      {
        "singleQuote": true,
        "semi": false,
        "trailingComma": "es5"
      }
    ]
  },
  "options": {
    "typeCheck": true
  }
}
